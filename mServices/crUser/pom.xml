<?xml version="1.0"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<url>http://maven.apache.org</url>

	<parent>
		<groupId>com.mk.mnx</groupId>
		<artifactId>mServices</artifactId>
		<version>1.0.0-SNAPSHOT</version>
	</parent>

	<artifactId>crUser</artifactId>
	<packaging>jar</packaging>
	<name>crUser Maven Webapp</name>

	<dependencies>
		<dependency>
			<groupId>com.mk.mnx</groupId>
			<artifactId>restSupport</artifactId>
			<version>1.0.0-SNAPSHOT</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<mainClass>com.mk.mnx.StartUpService</mainClass>
				</configuration>
			</plugin>
		</plugins>

		<finalName>crUser</finalName>
	</build>

</project>
